{
  "name": "@doccov/api-shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./env": {
      "types": "./src/env.ts",
      "default": "./src/env.ts"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "default": "./src/errors.ts"
    },
    "./middleware": {
      "types": "./src/middleware/index.ts",
      "default": "./src/middleware/index.ts"
    },
    "./middleware/rate-limit": {
      "types": "./src/middleware/rate-limit.ts",
      "default": "./src/middleware/rate-limit.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@doccov/db": "workspace:*",
    "@doccov/sdk": "workspace:*",
    "@openpkg-ts/spec": "workspace:*",
    "kysely": "^0.27.4",
    "nanoid": "^5.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
