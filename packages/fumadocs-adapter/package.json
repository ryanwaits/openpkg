{
  "name": "@doccov/fumadocs-adapter",
  "version": "0.1.0",
  "description": "Fumadocs integration for OpenPkg API documentation",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./components": "./src/components/index.ts",
    "./css": "./src/styles/docskit.css"
  },
  "scripts": {
    "build": "bunup",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@openpkg-ts/doc-generator": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "bunup": "^0.11.5",
    "typescript": "^5.0.0"
  }
}
