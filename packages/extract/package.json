{
  "name": "@openpkg-ts/extract",
  "version": "0.11.2",
  "description": "TypeScript export extraction to OpenPkg spec",
  "keywords": [
    "openpkg",
    "typescript",
    "api-extractor",
    "documentation",
    "type-extraction"
  ],
  "homepage": "https://github.com/doccov/doccov#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doccov/doccov.git",
    "directory": "packages/extract"
  },
  "license": "MIT",
  "author": "Ryan Waits",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tspec": "./dist/bin/tspec.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/"
  },
  "dependencies": {
    "@openpkg-ts/spec": "workspace:*",
    "commander": "^12.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "bunup": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
